 
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Clinic Pharmacy Admin - Dashboard</title>
    <!-- Custom fonts for this template-->
    <link href="~/Areas/Admin/sbadmin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <!-- Page level plugin CSS-->
    <link href="~/Areas/Admin/sbadmin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="~/Areas/Admin/sbadmin/css/sb-admin.css" rel="stylesheet">
</head>
<body id="page-top">
    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">
        <a class="navbar-brand mr-1" href="@Url.Action("Index","Dashboard")">Clinic Pharmacy Admin Panel</a>
        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle"href="~/Areas/Admin/sbadmin/#">
            <i class="fas fa-bars"></i>
        </button>
        <!-- Navbar Search -->
        <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
            <div class="input-group">
                 
            </div>
        </form>
        <!-- Navbar -->
        <ul class="navbar-nav ml-auto ml-md-0">
          
            <li class="nav-item dropdown no-arrow">
                <a class="nav-link dropdown-toggle"href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  @if (@Session["aname"] != null)
                  {  @Session["aname"].ToString() }<i class="fas fa-user-circle fa-fw"></i>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    <a class="dropdown-item"href="@Url.Action( "Home", "Home", new {Area = ""} )">Website</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item"href="@Url.Action("AdminLogout", "Login")"   >Logout</a>
                </div>
            </li>
        </ul>
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="sidebar navbar-nav">

            <li class="nav-item  ">
                <a class="nav-link" href="@Url.Action("Index","Dashboard")">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>



            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="~/Areas/Admin/sbadmin/#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-medkit"></i>
                    <span>Add Medicine</span>
                </a>
                 <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <h6 class="dropdown-header">Medicines:</h6>
                    <a class="dropdown-item " href="@Url.Action("AddMedicines", "Products")">Add Medicine</a>
                    <a class="dropdown-item" href="@Url.Action("AllMedicines", "Products")">All Medicines</a>
     
                </div>
            </li>



            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="~/Areas/Admin/sbadmin/#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-flask"></i>
                    <span>Add Apparatus</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                   <h6 class="dropdown-header">Scientific Apparatus:</h6>
                    <a class="dropdown-item" href="@Url.Action("AddApparatus", "Products")">Add Apparatus</a>
                    <a class="dropdown-item" href="@Url.Action("AllApparatus", "Products")">All Apparatus</a>
                </div>
            </li>
            




            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="~/Areas/Admin/sbadmin/#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-graduation-cap"></i>
                    <span>Add Events</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <h6 class="dropdown-header">Education Events:</h6>
                    <a class="dropdown-item" href="@Url.Action("AddEducation", "Products")">Add Events</a>
                    <a class="dropdown-item" href="@Url.Action("AllEducation", "Products")">All Events</a>
                </div>
            </li>


             
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="~/Areas/Admin/sbadmin/#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>Inventory</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <h6 class="dropdown-header">Inventory Record:</h6>
                    <a class="dropdown-item"  href="@Url.Action("AddInventory", "Inventory")">Add Inventory</a>
                    <a class="dropdown-item"  href="@Url.Action("AllInventory", "Inventory")">Current Inventory</a>
                </div>
            </li>

            <li class="nav-item ">
                <a class="nav-link" href="@Url.Action("Orders", "Reports")">
                    <i class="fas fa-fw fa-cart-plus"></i>
                    <span>Orders</span>
                </a>
            </li>

            <li class="nav-item ">
                <a class="nav-link" href="@Url.Action("ClientList", "Reports")">
                    <i class="fas fa-fw fa fa-users"></i>
                    <span>Clients Record</span>
                </a>
            </li>
            <li class="nav-item ">
                <a class="nav-link" href="@Url.Action("Feedbacks", "Reports")">
                    <i class="fas fa-fw fa fa-users"></i>
                    <span>Feedbacks</span>
                </a>
            </li>




            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Reports</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                   
                    
                    <h6 class="dropdown-header">Transactions:</h6>
                    <a class="dropdown-item" href="@Url.Action("Medtransactions", "Reports")">Medicine</a>
                    <div class="dropdown-divider"></div>
                   
                    <a class="dropdown-item" href="@Url.Action("Apptransactions", "Reports")">Apparatus</a>
                    <div class="dropdown-divider"></div>
                
                    <a class="dropdown-item" href="@Url.Action("Edutransactions", "Reports")">Events/Education</a>
                </div>
            </li>




         
        </ul>
        <div id="content-wrapper">

           @RenderBody()



        </div>
        <!-- /.content-wrapper -->

    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded"href="~/Areas/Admin/sbadmin/#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary"href="~/Areas/Admin/sbadmin/login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>
    <!-- Bootstrap core JavaScript-->
    <script src="~/Areas/Admin/sbadmin/vendor/jquery/jquery.min.js"></script>
    <script src="~/Areas/Admin/sbadmin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/Areas/Admin/sbadmin/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Page level plugin JavaScript-->
    <script src="~/Areas/Admin/sbadmin/vendor/chart.js/Chart.min.js"></script>
    <script src="~/Areas/Admin/sbadmin/vendor/datatables/jquery.dataTables.js"></script>
    <script src="~/Areas/Admin/sbadmin/vendor/datatables/dataTables.bootstrap4.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/Areas/Admin/sbadmin/js/sb-admin.min.js"></script>
    <!-- Demo scripts for this page-->
    <script src="~/Areas/Admin/sbadmin/js/demo/datatables-demo.js"></script>
    <script src="~/Areas/Admin/sbadmin/js/demo/chart-area-demo.js"></script>
    <script src="~/js/googlechart.js"></script>

</body>
</html>
